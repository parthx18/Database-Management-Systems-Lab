CREATE TABLE INSTRUCTOR (
    instructor_id NUMBER PRIMARY KEY,
    name VARCHAR2(50) NOT NULL,
    dept_id NUMBER,
    is_hod CHAR(1) DEFAULT 'N' CHECK (is_hod IN ('Y', 'N')),
    salary NUMBER(10, 2),
    CONSTRAINT fk_instructor_dept
        FOREIGN KEY (dept_id)
        REFERENCES DEPARTMENT(dept_id)
);
