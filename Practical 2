CREATE TABLE DEPARTMENT(dept_id, INT PRIMARY KEY,name VARCHAR(50)NOT NULL,building VARCHAR(50),budget DECIMAL(12,2),university_id INT,FOREIGN KEY(university_id)REFERENCES UNIVERSITY(university_id));
