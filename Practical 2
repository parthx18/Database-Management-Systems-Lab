CREATE TABLE TIME_SLOT(time_slot-id VARCHAR2(4) PRIMARY KEY,day CHAR(1),start_time DATE,end_time DATE );
INSERT INTO TIME_SLOT(time_slot_id,day,start_time,end_time)VALUES('A','M',TO_DATE('09:00','HH24:MI'),TO_DATE('12:30','HH24:MI'));


CREATE TABLE CLASSROOM(building VARCHAR(50),room_number VARCHAR(10),capacity INT,PRIMARY KEY(building,room_number));
